relator:oth mads:authoritativeLabel "Other"@en ;
	mads:code "oth" ;
	mads:definitionNote "A role that has no equivalent in the MARC list." .

relator:own mads:authoritativeLabel "Owner"@en ;
	mads:code "own" ;
	mads:definitionNote "A person, family, or organization that currently owns an item or collection, i.e.has legal possession of a specific copy or instance of a resource" .

relator:pan mads:authoritativeLabel "Panelist"@en ;
	mads:code "pan" ;
