relator:fpy mads:authoritativeLabel "First party"@en ;
	mads:code "fpy" ;
	mads:definitionNote "A person or organization who is identified as the only party or the party of the first party. In the case of transfer of rights, this is the assignor, transferor, licensor, grantor, etc. Multiple parties can be named jointly as the first party" .

relator:frg mads:authoritativeLabel "Forger"@en ;
	mads:code "frg" ;
	mads:definitionNote "A person or organization who makes or imitates something of value or importance, especially with the intent to defraud" .

relator:gis mads:authoritativeLabel "Geographic information specialist"@en ;
	mads:code "gis" ;
	mads:definitionNote "A person responsible for geographic information system (GIS) development and integration with global positioning system data" .

relator:grt mads:code "grt" .

relator:his mads:authoritativeLabel "Host institution"@en ;
