@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#>.
@prefix oslo:   <http://purl.org/oslo/ns/localgov#>.
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix dcterms:<http://purl.org/dc/terms/> .
@prefix adms:   <http://www.w3.org/ns/adms#>.
@prefix locn:   <http://www.w3.org/ns/locn#>.
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .


<#MARC2BIBFRAME_Item>

    rml:logicalSource [
        rml:source "marc_record" ;
        rml:referenceFormulation ql:JSON ;
        rml:iterator "$"

    ] ;

    rr:subjectMap [
        rr:template "{repository_iri}" ;
        rr:class bf:Item 
    ]; 

    rr:predicateObjectMap [
        rr:predicate bf:heldBy ;
        rr:objectMap [
            rr:constant <http://bibcat.org> 
        ]
    ] .

<#MARC2BIBFRAME_Instance> 

    rml:logicalSource [
        rml:source "marc_record" ;
        rml:referenceFormulation ql:JSONPath ;
        rml:iterator "$"
    ] ;


    rr:predicateObjectMap [
        rr:predicate bf:title ;
        rr:objectMap [
            rml:reference "$.fields.245.ind1=1/subfields.a"
            
        ] 
    ] .
