relator:prp mads:authoritativeLabel "Production place"@en ;
	mads:code "prp" ;
	mads:definitionNote "The place of production (e.g., inscription, fabrication, construction, etc.) of a resource in an unpublished form" .

relator:prs mads:authoritativeLabel "Production designer"@en ;
	mads:code "prs" ;
