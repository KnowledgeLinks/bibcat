relator:evp mads:authoritativeLabel "Event place"@en ;
	mads:code "evp" ;
	mads:definitionNote "A place where an event such as a conference or a concert took place" .

relator:exp mads:authoritativeLabel "Expert"@en ;
	mads:code "exp" ;
	mads:definitionNote "A person or organization in charge of the description and appraisal of the value of goods, particularly rare items, works of art, etc." .

relator:fac mads:authoritativeLabel "Facsimilist"@en ;
	mads:code "fac" ;
	mads:definitionNote "A person or organization that executed the facsimile" .

relator:fds mads:authoritativeLabel "Film distributor"@en ;
