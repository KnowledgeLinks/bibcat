@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix kds: <http://knowledgelinks.io/ns/data-structures/> .
@prefix kdr: <http://knowledgelinks.io/ns/data-resources/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix relator: <http://id.loc.gov/vocabulary/relators/> .
@prefix bc: <http://knowledgelinks.io/ns/bibcat/> .
@prefix m21.1: <http://marc21rdf.info/elements/1XX/> .
@prefix m21.2: <http://marc21rdf.info/elements/2XX/> .
@prefix m21.3: <http://marc21rdf.info/elements/3XX/> .
@prefix m21.4: <http://marc21rdf.info/elements/4XX/> .
@prefix m21.5: <http://marc21rdf.info/elements/5XX/> .
@prefix m21.6: <http://marc21rdf.info/elements/6XX/> .
@prefix m21.7: <http://marc21rdf.info/elements/7XX/> .
@prefix m21.8: <http://marc21rdf.info/elements/8XX/> .

bc:m21-bf_carrier a kds:PropertyLinker;
    kds:srcPropUri m21.3:M338__a,
                   m21.3:M338__b;
    kds:destClassUri bf:Carrier;  # the class the value will be saved in
    kds:destPropUri rdf:value;    # the property the value will be saved in
    kds:linkedRange bf:carrier;   # the property the resultant uri will be saved in
    kds:linkedClass bf:Instance.   # the class of the linked Range

bc:m21-bf_copyrightDate a kds:PropertyLinker;
    kds:srcPropUri m21.2:M260__c;
    kds:destClassUri bf:Instance;
    kds:destPropUri bf:copyrightDate.
    # a linkedRange and class are not required since the m21 value is not being saved a class first

bc:m21-bf_barcode a kds:PropertyLinker ;
	kds:srcPropUri m21.1:barcodeField
    kds:destClassUri bf:Item;
    kds:destPropUri bf:barcode.

bc:m21-bf_electronicLocation a kds:PropertyLinker ;
	kds:srcPropUri m21.8:M856__a ;
	kds:destClassUri bf:Item ;
	kds:destPropUri bf:electronicLocation. 

bc:m21-bf_InstanceTitle a a kds:PropertyLinker ;
	kds:srcPropUri 	m21.2:M24500a,
					m21.2:M24510a,
					m21.2:M24511a,
					m21.2:M24512a,
					m21.2:M24513a,
					m21.2:M24514a,
					m21.2:M24515a,
					m21.2:M24516a,
					m21.2:M24517a,
					m21.2:M24518a,
					m21.2:M24519a ;
	kds:destClassUri bf:InstanceTitle;
	kds:destPropUri bf:mainTitle;
	kds:linkedRange bf:title;
	kds:linkedClass	bf:Instance .

#----- Below Not Done yet -------------------

bc:m21-bf_originDate a owl:DatatypeProperty ;
    kds:marc2bibframe <http://marc21rdf.info/elements/00X/M00807-10>,
        <http://marc21rdf.info/elements/2XX/M2602_c>;
    rdfs:domain bf:Work;
    rdfs:range xsd:string .

bf:Topic a owl:Class;
    kds:marc2bibframe <http://marc21rdf.info/elements/65X/M650_0a>,
        <http://marc21rdf.info/elements/65X/M650_0b>,
        <http://marc21rdf.info/elements/65X/M650_0c>,
        <http://marc21rdf.info/elements/65X/M650_0x>;
    rdfs:domain bf:Work,
        bf:Instance,
        bf:Item;
    rdfs:range xsd:string .

relator:aut a owl:Class ;
    kds:marc2bibframe <http://marc21rdf.info/elements/1XX/M1000_a>,
        <http://marc21rdf.info/elements/1XX/M1001_a>,
        <http://marc21rdf.info/elements/1XX/M1003_a>;
    rdfs:domain bf:Person,
        bf:Organization,
        bf:Family,
        bf:Meeting;
    rdfs:range xsd:string,
        xsd:uri .

kds:PersonIdentifier a kds:Deduplicator;
    kds:exactMatch rdfs:value;
    rdfs:domain bf:Person .

kds:WorkIdentifier a kds:Deduplicator;
    kds:deDupeMethod [
    	a kds:exactMatch; 
    	kds:propUri bf:mainTitle,
        	    relator:aut 
    ];
    kds:deDupeMethod [
    	a kds:looseMatch; 
    	kds:propUri bf:mainTitle,
        	    relator:aut,
        	    relator:contributor;
   	kds:matchCriteria kds:2outOf3
    ];
    rdfs:domain bf:Work .
