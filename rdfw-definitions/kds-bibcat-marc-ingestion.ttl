@prefix bf: <http://id.loc.gov/ontologies/bibframe/> .
@prefix kds: <http://knowledgelinks.io/ns/data-structures/> .
@prefix kdr: <http://knowledgelinks.io/ns/data-resources/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix relator: <http://id.loc.gov/vocabulary/relators/> .
@prefix bc: <http://knowledgelinks.io/ns/bibcat/> .
@prefix m211: <http://marc21rdf.info/elements/1XX/> .
@prefix m212: <http://marc21rdf.info/elements/2XX/> .
@prefix m213: <http://marc21rdf.info/elements/3XX/> .
@prefix m214: <http://marc21rdf.info/elements/4XX/> .
@prefix m215: <http://marc21rdf.info/elements/5XX/> .
@prefix m216: <http://marc21rdf.info/elements/6XX/> .
@prefix m217: <http://marc21rdf.info/elements/7XX/> .
@prefix m218: <http://marc21rdf.info/elements/8XX/> .

bc:m21-bf_carrier a kds:PropertyLinker;
    kds:srcPropUri m213:M338__a,
    		   M338__b;
    kds:destClassUri bf:Carrier;  # the class the value will be saved in
    kds:destPropUri rdf:value;    # the property the value will be saved in
    kds:linkedRange bf:carrier;   # the property the resultant uri will be saved in
    kds:linkedClass bf:Instance.   # the class of the linked Range

bc:m21-bf_copyrightDate a kds:PropertyLinker;
    kds:srcPropUri m212:M260__c;
    kds:destClassUri bf:Instance;
    kds:destPropUri bf:copyrightDate.
    # a linkedRange and class are not required since the m21 value is not being saved a class first

#----- Below Not Done yet -------------------

bf:barcode a owl:DatatypeProperty ;
    rdfs:domain bf:Item;
    rdfs:range bf:Barcode .

bf:electronicLocation a owl:DatatypeProperty ;
    kds:marc2bibframe <http://marc21rdf.info/elements/8XX/M856__a;
    rdfs:domain bf:Item;
    rdfs:range xsd:string .

bf:mainTitle a owl:DatatypeProperty ;
    kds:marc2bibframe <http://marc21rdf.info/elements/2XX/M24500a> ,
        <http://marc21rdf.info/elements/2XX/M24510a>,
        <http://marc21rdf.info/elements/2XX/M24511a>,
        <http://marc21rdf.info/elements/2XX/M24512a>,
        <http://marc21rdf.info/elements/2XX/M24513a>,
        <http://marc21rdf.info/elements/2XX/M24514a>,
        <http://marc21rdf.info/elements/2XX/M24515a>,
        <http://marc21rdf.info/elements/2XX/M24516a>,
        <http://marc21rdf.info/elements/2XX/M24517a>,
        <http://marc21rdf.info/elements/2XX/M24518a>,
	<http://marc21rdf.info/elements/2XX/M24519a> ;
    rdfs:domain bf:Title,
        bf:WorkTitle,
        bf:InstanceTitle,
        bf:VariantTitle ;
    rdfs:range xsd:string .

bf:originDate a owl:DatatypeProperty ;
    kds:marc2bibframe <http://marc21rdf.info/elements/00X/M00807-10>,
        <http://marc21rdf.info/elements/2XX/M2602_c>;
    rdfs:domain bf:Work;
    rdfs:range xsd:string .

bf:Topic a owl:Class;
    kds:marc2bibframe <http://marc21rdf.info/elements/65X/M650_0a>,
        <http://marc21rdf.info/elements/65X/M650_0b>,
        <http://marc21rdf.info/elements/65X/M650_0c>,
        <http://marc21rdf.info/elements/65X/M650_0x>;
    rdfs:domain bf:Work,
        bf:Instance,
        bf:Item;
    rdfs:range xsd:string .

relator:aut a owl:Class ;
    kds:marc2bibframe <http://marc21rdf.info/elements/1XX/M1000_a>,
        <http://marc21rdf.info/elements/1XX/M1001_a>,
        <http://marc21rdf.info/elements/1XX/M1003_a>;
    rdfs:domain bf:Person,
        bf:Organization,
        bf:Family,
        bf:Meeting;
    rdfs:range xsd:string,
        xsd:uri .

kds:PersonIdentifir a kds:Deduplicator;
    kds:exactMatch rdfs:value;
    rdfs:domain bf:Person .

kds:WorkIdentifier a kds:Deduplicator;
    kds:deDupeMethod [
    	a kds:exactMatch; 
    	kds:propUri bf:mainTitle,
        	    relator:aut 
    ];
    kds:deDupeMethod [
    	a kds:looseMatch; 
    	kds:propUri bf:mainTitle,
        	    relator:aut,
        	    relator:contributor;
   	kds:matchCriteria kds:2outOf3
    ];
    rdfs:domain bf:Work .
