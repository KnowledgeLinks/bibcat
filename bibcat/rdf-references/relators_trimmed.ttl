relator:inv mads:authoritativeLabel "Inventor"@en ;
	mads:code "inv" ;
	mads:definitionNote "A person, family, or organization responsible for creating a new device or process" .

relator:isb mads:authoritativeLabel "Issuing body"@en ;
	mads:code "isb" ;
	mads:definitionNote "A person, family or organization issuing the work, such as an official organ of the body" .

relator:itr mads:authoritativeLabel "Instrumentalist"@en ;
	mads:code "itr" .

relator:opn mads:authoritativeLabel "Opponent"@en ;
	mads:code "opn" ;
	mads:definitionNote "A person or organization responsible for opposing a thesis or dissertation" .

relator:org mads:authoritativeLabel "Originator"@en ;
	mads:code "org" ;
	mads:definitionNote "A person or organization performing the work, i.e., the name of a person or organization associated with the intellectual content of the work. This category does not include the publisher or personal affiliation, or sponsor except where it is also the corporate author" .

relator:orm mads:authoritativeLabel "Organizer of meeting"@en ;
	mads:code "orm" ;
	mads:definitionNote "A person or organization responsible for organizing a meeting for which the resource is the report or proceedings" .

relator:osp mads:authoritativeLabel "Onscreen presenter"@en ;
