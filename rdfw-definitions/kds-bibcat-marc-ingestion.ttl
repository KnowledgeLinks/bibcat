kds:PersonIdentifier a kds:Deduplicator;
    kds:exactMatch rdf:value;
    rdfs:domain bf:Person .

kds:WorkIdentifier a kds:Deduplicator;
    kds:deDupeMethod [
    	a kds:exactMatch; 
    	kds:propUri bf:mainTitle,
        	    relators:aut 
    ];
    kds:deDupeMethod [
    	a kds:looseMatch; 
    	kds:propUri bf:mainTitle,
        	    relators:aut,
        	    relators:contributor;
   	kds:matchCriteria kds:2outOf3
    ];
    rdfs:domain bf:Work .
